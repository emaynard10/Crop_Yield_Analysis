CREATE TABLE pesticides (
	Domain VARCHAR,
	Area VARCHAR,
	Element VARCHAR,
	Item VARCHAR,
	Year INT,
	Unit VARCHAR,
	Value FLOAT)
	
CREATE TABLE yield (
	Domain_Code VARCHAR,
	Domain VARCHAR,
	Area_Code INT,
	Area VARCHAR,
	Element_Code INT,
	Element VARCHAR,
	Item_Code INT,
	Item VARCHAR,
	Year_Code INT,
	Year INT,
	Unit VARCHAR,
	Value INT);
	
CREATE TABLE temp (
	year INT,
	country VARCHAR,
	avg_temp FLOAT);